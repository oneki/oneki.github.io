{
  "name": "onekijs-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "typedoc-test": "typedoc --json .typedoc/test.json --tsconfig ../onekijs/packages/onekijs-ui/tsconfig.json ../onekijs/packages/onekijs-ui/src/index.ts",
    "typedoc:ui": "typedoc --json .typedoc/oneki-ui.json --tsconfig ../onekijs/packages/onekijs-ui/tsconfig.json ../onekijs/packages/onekijs-ui/src/index.ts",
    "typedoc:framework": "typedoc --json .typedoc/oneki-framework.json --tsconfig ../onekijs/packages/onekijs/tsconfig.json ../onekijs/packages/onekijs/src/index.ts",
    "api:ui": "ts-node ../onekijs/packages/onekijs-api/src/index.ts ./.typedoc/oneki-ui.json ./docs/api2 /api2",
    "api:framework": "ts-node ../onekijs/packages/onekijs-api/src/index.ts ./.typedoc/oneki-framework.json ./docs/framework-api /framework-api",
    "ui": "yarn typedoc:ui && yarn api:ui",
    "framework": "yarn typedoc:framework && yarn api:framework",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy"
  },
  "dependencies": {
    "@akebifiky/remark-simple-plantuml": "^1.0.1",
    "@docusaurus/core": "^2.0.0",
    "@docusaurus/preset-classic": "^2.0.0",
    "@saucelabs/theme-github-codeblock": "^0.1.1",
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typedoc": "^0.23.10",
    "url": "^0.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
